{
	"name": "@ct/valita-validator",
	"description": "A validator for hono with valita",
	"version": "0.0.1",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"files": [
		"src",
		"dist"
	],
	"types": "./dist/types/valita-validator.d.ts",
	"main": "./dist/valita-validator.cjs",
	"module": "./dist/valita-validator.js",
	"exports": {
		".": {
			"import": "./dist/valita-validator.js",
			"require": "./dist/valita-validator.cjs",
			"types": "./dist/types/valita-validator.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"fmt": "prettier src --write",
		"fmt:ci": "prettier src --check",
		"test": "vitest",
		"emit-types": "tsc --emitDeclarationOnly",
		"tc": "tsc --noEmit",
		"build": "vite build --mode production",
		"build-all": "pnpm build && pnpm emit-types"
	},
	"devDependencies": {
		"@vitest/coverage-c8": "^0.31.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.31.1"
	},
	"dependencies": {
		"@badrap/valita": "^0.2.0",
		"hono": "^3.2.2"
	},
	"peerDependencies": {
		"@badrap/valita": "^0.2.0",
		"hono": "^3.2.2"
	}
}
